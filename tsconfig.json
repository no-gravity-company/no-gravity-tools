{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "strict": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "noImplicitAny": true,
        "removeComments": true,
        "noLib": false,
        "experimentalDecorators": true,
        "baseUrl": "./",
        "noEmit": true
    },
    "include": ["/**/*.ts"]
}
